pipeline {
    agent 
    {
        label 'dotnet5_0'
    }	
    stages 
    {  
       stage('Checkout') 
       {    
           steps 
           {      
               git(url:'https://github.com/UWTSDStudents/jokeapi.git')
           }  
        }  
        stage('Compile') 
        {    
            steps 
            {      
                sh 'dotnet clean'      
                sh 'dotnet build'    
            }  
         }  
        stage('Test') 
        {    
            steps 
           {            
                sh 'dotnet test'    
            }  
        }
    }   
}
